
<%- include('./../sticky_html_start') %>
<div id="page">
    <!--div class="header header-fixed header-logo-center">
        <a href="index.html" class="header-title"><img src="/image/logo.png" class="max-auto mr-1 scale-box" height="30">ABC company</a>
        <a href="#" data-back-button class="header-icon header-icon-1"><i class="fas fa-arrow-left fa-3x" ></i></a>
        
    </div-->

    <!-- footer is removed-->

    <div class="page-content header-clear-medium w-50 " style="padding-top:50px;margin:auto;">
        <div class="card card-style mt-5" >
            <div class="content mt-4 mb-0">
                <h1 class="text-center font-900 font-40 text-uppercase mb-0">Login</h1>

                <div class="divider mt-3"></div>

                <p  class="bottom-0 text-center color-highlight font-11"><%= alerttext%></p>
                <form class="form-signin" action="/admin/signin" method="post" id="form-login-1">

                <div class="input-style has-icon input-style-1 input-required pb-1">
                    <i class="input-icon fa fa-at color-theme"></i>
                    <span>Email</span>
                    <em>(required)</em>
                    <input type="email" placeholder="Email address" name="email">
                </div> 
                <div class="input-style has-icon input-style-1 input-required pb-1">
                    <i class="input-icon fa fa-lock color-theme"></i>
                    <span>Password</span>
                    <em>(required)</em>
                    <input type="password" placeholder="Password" name="pazzword">
                </div> 
                
                <button class="btn btn-m mt-2 mb-4 btn-full bg-green1-dark text-uppercase font-900 btn-block" type="submit" id="data-login-submit">Login</button>
                </form>
                <div class="divider" hidden></div>

                <a href="#" class="btn btn-icon btn-m btn-full shadow-l bg-facebook text-uppercase font-900 text-left" hidden><i class="fab fa-facebook-f text-center"></i>Login with Facebook</a>
                <a href="#" class="btn btn-icon btn-m mt-2 btn-full shadow-l bg-twitter text-uppercase font-900 text-left" hidden><i class="fab fa-twitter text-center"></i>Login with Twitter</a>

                <div class="divider mt-4 mb-3" hidden></div>

                <div class="d-flex" hidden>
                    <div hidden class="w-50 font-11 pb-2 color-theme opacity-60 pb-3 text-left"><a href="#" class="color-theme">Create Account</a></div>
                    <div hidden class="w-50 font-11 pb-2 color-theme opacity-60 pb-3 text-right"><a href="#" class="color-theme">Forgot Credentials</a></div>
                </div>
            </div>
        </div>
</div>
<%- include('./../sticky_html_end') %>
<script>
    $("#data-login-submit").click(()=>{
        console.log("click");
        $("#form-login-1").submit();
    });
</script>
</body>
</html>






